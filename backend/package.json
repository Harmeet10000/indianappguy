{
  "name": "auth_service_backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=22.14.0",
    "npm": ">=10.7.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon src/index.js",
    "build": "rm -rf dist && cross-env NODE_ENV=production npx webpack --config webpack.config.js",
    "start": "node --env-file=.env.development dist/index.cjs",
    "swagger": "node ./docs/swagger.js",
    "test": "node test-runner.js",
    "test:watch": "node --watch test-runner.js",
    "test:coverage": "node --test --coverage test-runner.js",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "migrate:gen": "pnpm run db:generate",
    "migrate:exec": "pnpm run db:migrate",
    "seed": "pnpm run db:seed",
    "studio": "pnpm run db:studio",
    "mig:push": "pnpm run db:push",
    "backup:start": "node scripts/dbBackup.js",
    "backup:once": "cross-env RUN_BACKUP_ONCE=true node scripts/dbBackup.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "node src/db/migrate.js",
    "db:seed": "node src/db/seeders/index.js",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:drop": "drizzle-kit drop",
    "security:test": "snyk test",
    "security:monitor": "snyk monitor",
    "security:fix": "snyk fix",
    "security:wizard": "snyk wizard",
    "snyk:setup": "bash scripts/snyk-setup.sh"
  },
  "keywords": [],
  "author": "Harmeet Singh",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Harmeet10000/production-grade-auth-template.git"
  },
  "bugs": {
    "url": "https://github.com/Harmeet10000/production-grade-auth-template/issues"
  },
  "homepage": "https://github.com/Harmeet10000/production-grade-auth-template#readme",
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "npm run format:fix"
    ]
  },
